@{
    ViewBag.Title = "污染点";
    ViewData["dataSelector"] = ".js-lists";
    Layout = "~/Views/Shared/_BasicLayout.cshtml";

    var jsFiles = new List<string>();
    jsFiles.Add("Scripts/common/dataHandler.js");
    jsFiles.Add("Scripts/common/charts.js");
    jsFiles.Add("Scripts/common/list.js");
    jsFiles.Add("Scripts/plugins/slimscroll/jquery.slimscroll.min.js");
    jsFiles.Add("Scripts/plugins/heighChart/highcharts.js");
    jsFiles.Add("Scripts/init.js");
    jsFiles.Add("Scripts/pollution.js");
    jsFiles.Add("Scripts/plugins/metisMenu/jquery.metisMenu.js");
    jsFiles.Add("Scripts/plugins/layer/layer.min.js");
    jsFiles.Add("Scripts/hplus.min.js");
    jsFiles.Add("Scripts/jquery.layout.js");
    jsFiles.Add("Scripts/ztree/js/jquery.ztree.core.js");
    ViewBag.JsModules = jsFiles;
    var csFiles = new List<string>();
    csFiles.Add("Content/Common.css");
    csFiles.Add("Scripts/ztree/css/zTreeStyle/zTreeStyle.css");
    csFiles.Add("Content/framework-ui.css");
    ViewBag.cssModules = csFiles;
}
<script>
    $(function () {
        $('#layout').layout();
    });
</script>

