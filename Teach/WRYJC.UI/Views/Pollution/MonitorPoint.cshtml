<div class="ui-layout" id="layout" style="height: 94%; width: 100%;">
    @* 左侧内容 *@
    <div class="ui-layout-west">
        <div id="tab-contentdiv" class="tab-content" style="height: 97%;width:95%">
            @* 上方搜索内容 *@
            <form class="form-horizontal m-t js-search" id="commentForm" name="commentForm" novalidate="novalidate">
                <div class="row">
                    <div class="form-group col-sm-12">
                        <label class="col-md-5 col-lg-5 control-label">名称：</label>
                        <div class="col-md-7 col-lg-7">
                            <input name="Name" minlength="2" type="text" class="form-control">
                        </div>
                    </div>
                    <div class="form-group col-sm-12">
                        <label class="col-md-5 col-lg-5 control-label">环保联系人：</label>
                        <div class="col-md-5 col-lg-5">
                            <input name="Contact" type="text" class="form-control">
                        </div>
                        <div class="form-group col-sm-2">
                            <button class="btn btn-primary" type="submit">查询</button>
                        </div>
                    </div>
                </div>
            </form>
            @* 搜索以下内容 *@
            <div id="tab-1" class="tab-pane active" style="width: 100%; height: 68%;">
                @* 树形结构内容 *@
                <div class="js-lists full-height-scroll" style="height:80%;" url="@Url.Action("EnterpriseTree", "Pollution")">
                    <ul id="treeDemo" class="ztree"></ul>
                </div>
                @* 下方脚注及翻页按钮 *@
                <div class="row js-footer">
                    <div class="row right">
                        总共${total}条,共${rows}页，当前第${page}页
                    </div>
                    <br />
                    <div class="ibox-tools ">
                        @*<div class="btn-group js-page" selector="操作列表的名称" listType="列表的类型"></div>*@
                        <div class="btn-group js-page" selector=".table" listtype="table"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @* 中间内容 *@
    <div class="ui-layout-center">
        <input id="hcurrentid" type="hidden" />
        <input id="hcurrentfun" type="hidden" />

        @* 选择企业污染点 *@
        <div class="ibox" style="height:96%" id="erealtime" hidden="hidden">
            <div class="ibox-content no-top-border float-e-margins" style="height:95%;">
                <div class="row">
                    <input id="hrealtimeid" type="hidden" />
                    <div class="form-group col-sm-12">
                        <div class="col-md-4 col-lg-4 control-label" id="EName">请选择企业</div>
                        <label class="col-md-2 col-lg-2 control-label">污染点：</label>
                        <div class="col-md-6 col-lg-6" id="WasteObjectLists">
                            @*@Html.DropDownList("WasteObjectLists", WasteObjectList, new { @placeholder = "性别...", @class = "chosen-select-width col-sm-11", @id = "WasteObjectLists" })*@
                        </div>
                    </div>
                </div>

                <div class="ibox-content">
                    <div class="row">
                        <div class="ibox float-e-margins" style="width: 97%;overflow-y:auto;">
                            <div id="chartShow" class="row" style="width:90%">
                                <div id="linechart0" class="js-chart line-container col-md-4 col-lg-4"></div>
                                <div id="linechart1" class="js-chart line-container col-md-4 col-lg-4"></div>
                                <div id="linechart2" class="js-chart line-container col-md-4 col-lg-4"></div>
                                <div id="linechart3" class="js-chart line-container col-md-4 col-lg-4"></div>
                                <div id="linechart4" class="js-chart line-container col-md-4 col-lg-4"></div>
                                <div id="linechart5" class="js-chart line-container col-md-4 col-lg-4"></div>
                                <div id="linechart6" class="js-chart line-container col-md-4 col-lg-4"></div>
                                <div id="linechart7" class="js-chart line-container col-md-4 col-lg-4"></div>
                                <div id="linechart8" class="js-chart line-container col-md-4 col-lg-4"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        @* 企业首页 *@
        <div class="ibox" style="height:96%" id="eenterprise" hidden="hidden">
            <div class="ibox-content no-top-border float-e-margins" style="height:100%;">
                @* 最上方企业名称内容 *@
                <div class="row">
                    <label class="col-md-2 col-lg-2 control-label">企业:</label>
                    <div class="col-md-10 col-lg-10" id="EeName">请选择企业</div>
                </div>
                <div class="row">
                    <label class="col-md-2 col-lg-2 control-label">关注</label>
                    <div class="col-md-4 col-lg-4" id="EAttention"></div>
                    <label class="col-md-2 col-lg-2 control-label">状态</label>
                    <div class="col-md-4 col-lg-4" id="EState"></div>
                </div>

                <div class="ibox-content">
                    @* 头部设置信息 *@
                    <div class="row">
                        <form class="form-horizontal m-t js-search" id="commentForm" novalidate="novalidate">
                            <input id="ehid" name="ehid" hidden="hidden" />
                            <div class="form-group col-sm-3">
                                @Html.DropDownList("Status", new List<SelectListItem>(){new SelectListItem(){Text="日数据", Value="1"},
                            new SelectListItem(){Text="小时数据", Value="2"},new SelectListItem(){Text="十分钟数据", Value="3"}
                            }, new { @placeholder = "业务状态...", @class = "chosen-select-width col-sm-11" })
                            </div>
                            <div class="input-daterange form-group col-sm-11">
                                起止时间
                                <input type="text" class="input-sm " name="minInsertTime" />
                                -
                                <input type="text" class="input-sm " name="maxInsertTime" />
                            </div>
                            <div class="form-group col-sm-1">
                                <button class="btn btn-primary" type="submit">查询</button>
                            </div>
                        </form>
                    </div>
                    @* 表格信息 *@
                    <div class="row">
                        <table class="js-table table table-hover" url="@Url.Action("getReportRate", "Pollution")">
                            <thead>
                                <tr>
                                    <th>监控点</th>
                                    <th>数采仪</th>
                                    <th>应报记录数</th>
                                    <th>实报记录数</th>
                                    <th>上报率</th>
                                </tr>
                            </thead>
                            <tbody class="js-body">
                                <tr class="js-template">
                                    <td class="js-item" name="WasteObjectName">${WasteObjectName}</td>
                                    <td class="js-item" name="DataCollectCode">${DataCollectCode}</td>
                                    <td class="js-item" name="WasteObjectName">0</td>
                                    <td class="js-item" name="WasteObjectName">0</td>
                                    <td class="js-item" name="WasteObjectName">0</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    @* 下方信息 *@
                    <div class="row js-footer">
                        总共${total}条,共${rows}页，当前第${page}页
                        <div class="ibox-tools">
                            @*<div class="btn-group js-page" selector="操作列表的名称" listType="列表的类型"></div>*@
                            <div class="btn-group js-page" selector=".table" listtype="table"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        @* 监测数据 *@
        <div class="ibox" style="height:96%" id="emonitordata" hidden="hidden">
            <div class="ibox-content no-top-border float-e-margins" style="height:100%;">
                <div class="row">
                    <div class="form-group col-sm-12">

                        <label class="col-md-2 col-lg-3 control-label">企业名称</label>
                        <div class="col-md-3 col-lg-9" id="EmdName">请选择企业</div>

                    </div>

                </div>
                <div class="ibox-content">
                    <div class="row">
                        <form class="form-horizontal m-t js-search" id="commentForm" novalidate="novalidate">
                            <input id="ehid" name="ehid" hidden="hidden" />
                            <div class="form-group col-sm-3">
                                @Html.DropDownList("Status", new List<SelectListItem>(){new SelectListItem(){Text="日汇总", Value="1"},
                            new SelectListItem(){Text="月汇总", Value="2"},new SelectListItem(){Text="年汇总", Value="3"}
